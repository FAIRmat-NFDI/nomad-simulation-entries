{
  "version": "1.0",
  "description": "Index of NOMAD API queries used to collect simulation entries for parser testing",
  "queries": {
    "vasp_dos_bandstructure": {
      "description": "VASP entries with both electronic DOS and band structure data, representing comprehensive electronic structure calculations",
      "nomad_query_body": {
        "owner": "visible",
        "query": {
          "results.method.simulation.program_name": "VASP",
          "results.properties.available_properties:all": [
            "dos_electronic",
            "band_structure_electronic"
          ]
        }
      },
      "url_equivalent": "results.method.simulation.program_name=VASP&results.properties.available_properties=dos_electronic&results.properties.available_properties=band_structure_electronic",
      "used_in": ["VASP.jsonl"],
      "date": "2025-01-30",
      "total_entries_found": 4415529,
      "entries_selected": 10,
      "notes": "Selected diverse materials from AFLOW and Alexandria datasets with spin-polarized and non-spin-polarized calculations"
    },
    "fhiaims_dos": {
